<h1>Messages#send</h1>

<table class="table">
	<thead>
		<th>ID</th>
		<th>User ID</th>
		<th>Text</th>
	</thead>
	<% @messages.each do |message| %>
		<tr>
			<td><%= message.id %></td>
			<td><%= message.chat_id %></td>
			<td><%= message.text %></td>
		</tr>
	<% end %>
</table>

<%= link_to 'Create new message', new_message_path, class: 'btn btn-primary' %>